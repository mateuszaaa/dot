Description='A wireless connection using a custom network block configuration'
Interface=wlo1
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
    'ssid="NOSI"'
    'scan_ssid=1'
    'key_mgmt=WPA-EAP'
    'group=CCMP TKIP'
    'eap=TLS'
    'identity="host/matnowak.ipa.nsn-net.net"'
    'ca_cert="/home/cert/ca_bundle.crt"'
    'client_cert="/home/cert/matnowak.ipa.nsn-net.net.crt"'
    'private_key="/home/cert/matnowak.ipa.nsn-net.net.key"'
    'private_key_passwd="light"'
)
